# Si l'application doit être lancée en plein écran.
fullscreen=false
# Largeur de la fenêtre principale si n'est pas en plein écran.
width=1280
# Hauteur de la fenêtre principale si n'est pas en plein écran.
height=600
# Port par défaut à utiliser entre le serveur et le client.
port=42000

# Style de terminal sur lequel se baser.
# Valeurs possibles : /arch.css /ubuntu.css
terminal=/arch.css

# Couleur du curseur dans le terminal.
caret=#fff


# Toutes les couleurs suivantes écrasent celles définies dans le style du terminal
# si la valeur indiquée n'est pas none. Les couleurs possibles sont de la forme
# r,b,g
# fff (3 caractères en hexa)
# aabbcc (6 caractères en hexa)

# Couleur du texte dans le terminal.
terminal-foreground=#17a88b
# Couleur de fond du terminal.
terminal-background=#000

# Les couleurs suivantes sont utilisées pour afficher différentes choses.
terminal-Black=#000

terminal-Red=rgb(205,0,0)

terminal-Green=rgb(0,0,128)

terminal-Yellow=rgb(205,205,0)

terminal-Blue=rgb(0,0,238)

terminal-Magenta=rgb(205,0,205)

terminal-Cyan=rgb(0,205,205)

terminal-White=rgb(229,229,229)

terminal-Bright-Black=rgb(127,127,127)

terminal-Bright-Red=#f00

terminal-Bright-Green=#0f0

terminal-Bright-Yellow=#ff0

terminal-Bright-Blue=rgb(92,92,255)

terminal-Bright-Magenta=#f0f

terminal-Bright-Cyan=#0ff

terminal-Bright-White=#fff
